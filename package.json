{
  "name": "stark-orchestrator",
  "version": "0.0.1",
  "description": "Isomorphic JavaScript orchestration platform for Node.js and browser runtimes",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "build:shared": "pnpm --filter @stark-o/shared run build",
    "build:core": "pnpm --filter @stark-o/core run build",
    "build:server": "pnpm --filter @stark-o/server run build",
    "build:cli": "pnpm --filter @stark-o/cli run build",
    "dev": "pnpm -r --parallel run dev",
    "dev:server": "pnpm --filter @stark-o/server run dev",
    "lint": "eslint packages/*/src --ext .ts",
    "lint:fix": "eslint packages/*/src --ext .ts --fix",
    "format": "prettier --write \"packages/*/src/**/*.ts\"",
    "format:check": "prettier --check \"packages/*/src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf packages/*/dist node_modules/.cache",
    "prepare": "pnpm -r run build",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db reset --no-seed"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "orchestrator",
    "kubernetes",
    "isomorphic",
    "javascript",
    "typescript",
    "nodejs",
    "browser",
    "runtime"
  ],
  "license": "MIT"
}
