{
  "name": "stark-orchestrator",
  "version": "1.0.0",
  "description": "Orchestrator engine based on Pocketbase.",
  "bin": {
    "stark-db": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && node shebang.js",
    "build-clean": "npm run clean && npm run build",
    "clean": "node clean.js dist build package",
    "start": "node dist/index.js",
    "test": "set TS_NODE_PROJECT=tsconfig.test.json&& mocha -r ts-node/register test/unit/**/*.ts",
    "test-integration": "set TS_NODE_PROJECT=tsconfig.test.json&& mocha -r ts-node/register test/integration/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeWatchWall/stark-db"
  },
  "bugs": {
    "url": "https://github.com/WeWatchWall/stark-db/issues"
  },
  "keywords": [
    "database",
    "SQL",
    "Pocketbase"
  ],
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "license": "MIT",
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "async-retry": "^1.3.3",
    "async-sema": "^3.1.1",
    "commander": "^13.1.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "http-proxy": "^1.18.1",
    "memorystore": "^1.6.7",
    "pinia": "^3.0.2",
    "pocketbase": "^0.26.0",
    "selfsigned": "^2.4.1",
    "swagger-ui-express": "^5.0.1",
    "vue": "^3.5.13",
    "workerpool": "^9.2.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/chai": "^5.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/http-proxy": "^1.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.1",
    "@types/sinon": "^17.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "chai": "^5.2.0",
    "mocha": "^10.8.2",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
