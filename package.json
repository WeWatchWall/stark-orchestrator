{
  "name": "stark-os",
  "version": "0.10.8",
  "description": "JavaScript operating system and orchestration platform for Node.js and browser runtimes",
   "keywords": [
    "cloud",
    "javascript",
    "typescript",
    "devops",
    "orchestrator",
    "isomorphic",
    "nodejs",
    "browser"
  ],
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeWatchWall/stark-os.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "stark": "./packages/cli/dist/index.js"
  },
  "files": [
    "packages/*/dist/**",
    "packages/*/package.json",
    "scripts/postinstall.js",
    "README.md"
  ],
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "build": "pnpm -r run build",
    "build:shared": "pnpm --filter @stark-o/shared run build",
    "build:core": "pnpm --filter @stark-o/core run build",
    "build:server": "pnpm --filter @stark-o/server run build",
    "build:cli": "pnpm --filter @stark-o/cli run build",
    "build:client": "pnpm --filter @stark-o/client run generate",
    "dev": "pnpm -r --parallel run dev",
    "dev:server": "pnpm --filter @stark-o/server run dev",
    "dev:test": "pnpm --filter @stark-o/server run chaos -k",
    "lint": "eslint packages/*/src --ext .ts",
    "lint:fix": "eslint packages/*/src --ext .ts --fix",
    "format": "prettier --write \"packages/*/src/**/*.ts\"",
    "format:check": "prettier --check \"packages/*/src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf packages/*/dist node_modules/.cache",
    "prepare": "pnpm -r run build",
    "prepublishOnly": "pnpm -r run build",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db reset --no-seed"
  },
  "dependencies": {
    "@casl/ability": "^6.7.5",
    "@supabase/supabase-js": "^2.45.0",
    "@vue/reactivity": "^3.5.0",
    "@zenfs/core": "~2.4.4",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "node-cache": "^5.1.2",
    "selfsigned": "^2.4.1",
    "@roamhq/wrtc": "^0.9.1",
    "simple-peer": "^9.11.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  },
  "license": "MIT"
}
