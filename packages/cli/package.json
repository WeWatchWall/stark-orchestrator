{
  "name": "@stark-o/cli",
  "version": "0.0.1",
  "description": "Command-line interface for Stark Orchestrator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "stark": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@stark-o/core": "workspace:*",
    "@stark-o/node-runtime": "workspace:*",
    "@stark-o/server": "workspace:*",
    "@stark-o/shared": "workspace:*",
    "@supabase/supabase-js": "^2.45.0",
    "commander": "^13.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.5.0"
  },
  "files": [
    "dist"
  ],
  "license": "MIT"
}
