{
  "name": "stark-browser-config",
  "version": "0.1.0",
  "description": "Browser client bootstrap package for the Stark distributed orchestrator.",
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "private": true,
  "license": "MIT",
  "keywords": [
    "typescript",
    "esbuild"
  ],
  "browser": "./browser.js",
  "scripts": {
    "build:dev": "esbuild src/browser.ts --bundle --outfile=dist/browser.js",
    "build:watch": "esbuild src/browser.ts --bundle --watch --outfile=dist/browser.js",
    "postinstall": "node build.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "browser-env-vars": "^1.1.1",
    "esbuild": "^0.11.23",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "browser-assert": "^1.2.1",
    "deep-object-diff": "^1.1.0",
    "email-validator": "^2.0.4",
    "events": "^3.3.0",
    "flat-promise": "^1.0.2",
    "generate-password-browser": "^1.1.0",
    "objectmodel": "^4.3.0",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-security-helper": "^2.1.2",
    "promise-retry": "^2.0.1",
    "relational-pouch": "^4.0.0"
  }
