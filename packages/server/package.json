{
  "name": "@stark-o/server",
  "version": "0.0.1",
  "description": "Orchestrator server with REST API and WebSocket for Stark",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
