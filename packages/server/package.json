  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-proxy": "^1.17.17",
    "@types/node": "^20.14.0",
    "@types/selfsigned": "^2.0.4",
    "@types/ws": "^8.5.12",
    "http-proxy": "^1.18.1",
